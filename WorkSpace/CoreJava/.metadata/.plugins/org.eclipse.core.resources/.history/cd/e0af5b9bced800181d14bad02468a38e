package com.indus.training.height.impl;

import com.indus.training.height.IHeight;

public class Height implements IHeight {

	public double inchToFeet(double itof) {
		double feet = 0.0;

		feet = itof / 12.0;

		return feet;
	}

	public double feetToInch(double ftoi) {
		double inch = 0.0;

		inch = ftoi * 12.0;

		return inch;
	}

}
